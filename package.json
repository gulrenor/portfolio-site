{
  "name": "portfolio-site",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run watch",
    "build": "npm-run-all build:*",
    "build:scss": "node-sass src/sass/default.scss -o www/css/",
    "build:pug": "pug src/pug/index.pug -o www/",
    "build:ugly": "uglifycss www/css/default.css > www/css/default.min.css",
    "build:js": "concat www/js/scripts.js src/js/*.js",
    "build:minifyjs": "npm run build:js && minify www/js/scripts.js > www/js/scripts.min.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:scss": "npm run build:scss -- -w",
    "watch:pug": "npm run build:pug -- -w",
    "watch:js": "npm run build:js -- -w"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concat": "^1.0.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-bg": "0.0.8",
    "gulp-concat": "^2.6.1",
    "gulp-connect-php": "^1.0.3",
    "gulp-cssnano": "^2.1.3",
    "gulp-exec": "^2.1.3",
    "gulp-livereload": "^3.8.1",
    "gulp-plumber": "^1.2.1",
    "gulp-pug": "^3.3.0",
    "gulp-rename": "^1.4.0",
    "gulp-run": "^1.7.1",
    "gulp-sass": "^3.2.1",
    "gulp-uglify": "^2.1.2",
    "html": "^1.0.0",
    "minify": "^5.1.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "pug": "^2.0.4",
    "pug-cli": "^1.0.0-alpha6",
    "uglifycss": "0.0.29"
  },
  "dependencies": {
    "express": "^4.17.1",
    "opn": "^6.0.0"
  }
}
